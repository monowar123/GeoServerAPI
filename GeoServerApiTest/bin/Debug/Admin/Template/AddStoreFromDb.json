{
	"dataStore": {
		"name": "#datastore_name#",
		"description": "#datastore_name#",
		"type": "PostGIS",
		"enabled": true,
		"workspace": {
			"name": "#workspace_name#",
			"href": "#API_BASE_URL#workspaces/#workspace_name#.json"
		},
		"connectionParameters": {
			"entry": [{
				"@key": "schema",
				"$": "public"
			}, {
				"@key": "Evictor run periodicity",
				"$": "300"
			}, {
				"@key": "Max open prepared statements",
				"$": "50"
			}, {
				"@key": "encode functions",
				"$": "false"
			}, {
				"@key": "Batch insert size",
				"$": "1"
			}, {
				"@key": "preparedStatements",
				"$": "false"
			}, {
				"@key": "database",
				"$": "#db_name#"
			}, {
				"@key": "host",
				"$": "#host_name#"
			}, {
				"@key": "Loose bbox",
				"$": "true"
			}, {
				"@key": "Estimated extends",
				"$": "true"
			}, {
				"@key": "fetch size",
				"$": "1000"
			}, {
				"@key": "Expose primary keys",
				"$": "false"
			}, {
				"@key": "validate connections",
				"$": "true"
			}, {
				"@key": "Support on the fly geometry simplification",
				"$": "true"
			}, {
				"@key": "Connection timeout",
				"$": "20"
			}, {
				"@key": "create database",
				"$": "false"
			}, {
				"@key": "port",
				"$": "#port#"
			}, {
				"@key": "passwd",
				"$": "#password#"
			}, {
				"@key": "min connections",
				"$": "1"
			}, {
				"@key": "dbtype",
				"$": "postgis"
			}, {
				"@key": "namespace",
				"$": "http://localhost:8080/geoserver/#workspace_name#"
			}, {
				"@key": "max connections",
				"$": "10"
			}, {
				"@key": "Evictor tests per run",
				"$": "3"
			}, {
				"@key": "Test while idle",
				"$": "true"
			}, {
				"@key": "user",
				"$": "#user_id#"
			}, {
				"@key": "Max connection idle time",
				"$": "300"
			}]
		},
		"_default": false,
		"featureTypes": "#API_BASE_URL#workspaces/#workspace_name#/datastores/#datastore_name#/featuretypes.json"
	}
}